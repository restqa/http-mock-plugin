{
  "name": "http-mock-plugin",
  "version": "0.0.1",
  "description": "RestQA plugin enabling http mock",
  "main": "src/index.js",
  "dependencies": {
    "@restqa/plugin": "0.0.3",
    "testcontainers": "^7.21.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "got": "^11.8.2",
    "jest": "^27.2.4",
    "jest-runner-eslint": "^0.11.1",
    "jest-runner-prettier": "^0.3.7",
    "jsdoc-to-markdown": "^7.0.1",
    "prettier": "^2.4.1"
  },
  "scripts": {
    "lint": "eslint src example",
    "lint:fix": "eslint src example --fix",
    "format": "prettier --check src example",
    "format:fix": "prettier --write src example",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "example": "cucumber-js --require ./example/setup.js --publish-quiet --fail-fast ./example/features"
  },
  "keywords": [],
  "author": "",
  "jest": {
    "projects": [
      {
        "displayName": {
          "name": "test",
          "color": "yellow"
        }
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "eslint",
        "testMatch": [
          "<rootDir>/src/**/*.js"
        ]
      },
      {
        "runner": "jest-runner-prettier",
        "displayName": {
          "name": "prettier",
          "color": "blue"
        }
      }
    ]
  },
  "license": "ISC"
}
